<p id="notice"><%= notice %></p>

<h1>Listing Foods</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Food</th>
      <th>Expires</th>
      <th>Country</th>
      <th>State</th>
      <th>City</th>
      <th>Mobile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foods.each do |food| %>
      <tr>
        <td><%= food.name %></td>
        <td><%= food.food %></td>
        <td><%= food.expires %></td>
        <td><%= food.country %></td>
        <td><%= food.state %></td>
        <td><%= food.district %></td>
        <td><%= food.city %></td>
        <td><%= food.mobile %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food', new_food_path %>
